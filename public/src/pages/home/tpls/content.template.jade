md-content(layout="column")
    md-input-container.md-block.no-errors
        label 搜索图标
        input(ng-model="contentCtl.filter")
    div(angular-terminal="contentCtl.text")
    md-content(flex)
        md-button(ng-click="contentCtl.doOpenIconInfo($event,icon)",ng-repeat="icon in contentCtl.icons | filter:contentCtl.filter | orderBy | limitTo:50 ")
            div(layout="column")
                md-icon
                    ng-md-icon(icon="{{::icon}}")
                span {{::icon}}
