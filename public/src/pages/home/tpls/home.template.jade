//-     div.md-toolbar-tools(fx-toolbar,items="homeCtl.toolbar",layout="row",ctls="homeCtl")
md-content(flex="100",layout="row")
    md-sidenav.md-sidenav-left(ui-view="sidenavLeft",md-is-locked-open="$root.isOpenMenu",md-component-id="left",md-whiteframe="4",layout="column")
    md-sidenav.md-sidenav-right(md-component-id="right",ui-view="sidenavRight",layout="column",md-whiteframe="4")
    md-content(ui-view="content",flex="100",layout="column")
    div(execute-cmd,real-time="true")
    div.lock-size(ng-if="!homeCtl.isOpenMenu")
        md-fab-speed-dial.md-scale.md-fab-bottom-left(md-direction="right",ng-mouseenter="$root.isOpenFab=true",ng-mouseleave="$root.isOpenMFab=false",md-open="$root.isOpenFab")
            md-fab-trigger
                md-button.md-fab.md-default(aria-label="menu",ng-click="$root.isOpenMenu=true;")
                    md-icon
                        ng-md-icon(icon="menu")
            md-fab-actions
                div(fx-toolbar,layout="row",items="homeCtl.toolbars",ctls="homeCtl")